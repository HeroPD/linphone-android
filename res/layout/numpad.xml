<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="match_parent"
	android:layout_height="match_parent"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	android:background="@android:color/white"
	android:orientation="vertical">

	<LinearLayout
		style="@style/RowContainer"
		android:orientation="vertical" >

		<LinearLayout style="@style/RowContainer" >

			<LinearLayout style="@style/DigitContainer" >

				<io.karim.MaterialRippleLayout
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					app:mrlRippleColor="@color/main_app_effect_color" >

					<org.linphone.ui.Digit
						android:id="@+id/Digit1"
						android:layout_width="match_parent"
						android:layout_height="match_parent"
						android:background="@null"
						android:soundEffectsEnabled="true"
						android:text="1"
						android:textColor="@android:color/black"
						android:textSize="30sp" />
				</io.karim.MaterialRippleLayout>
			</LinearLayout>

			<LinearLayout style="@style/SeperatorGreyHorizontal" />

			<LinearLayout style="@style/DigitContainer" >

				<io.karim.MaterialRippleLayout
					android:layout_width="match_parent"
					android:layout_height="match_parent"
