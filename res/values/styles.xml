	<style name="AppBar" parent="@style/Theme.AppCompat.Light.NoActionBar">
		<item name="selectableItemBackground">@null</item>
	</style>
	<style name="FlatButtonRippleStyle" parent="Material.Drawable.Ripple.Touch.Light">
		<item name="android:background">@null</item>
		<item name="rd_cornerRadius">0dp</item>
	</style>
	<style name="FlatColorButtonRippleStyle" parent="Material.Drawable.Ripple.Touch.Light">
		<item name="android:background">@null</item>
		<item name="rd_backgroundColor">@color/main_app_effect_color</item>
		<item name="rd_rippleColor">@color/main_app_effect_color</item>
		<item name="rd_cornerRadius">0dp</item>
	</style>
	<style name="RaiseWaveButtonRippleStyle" parent="Material.Drawable.Ripple.Wave.Light">
		<item name="android:background">@drawable/bg_bt_raise</item>
		<item name="rd_leftPadding">1dp</item>
		<item name="rd_topPadding">1dp</item>
		<item name="rd_rightPadding">1dp</item>
		<item name="rd_bottomPadding">2dp</item>
	</style>
	<style name="RadioButtonDrawable" parent="Material.Drawable.RadioButton" />

	<style name="RadioButton">
		<item name="android:textColor">@drawable/button_text_color</item>
		<item name="android:minHeight">33dp</item>
		<item name="android:minWidth">70dp</item>
		<item name="android:gravity">center</item>
		<item name="android:button">@null</item>
		<item name="android:paddingLeft">5dp</item>
		<item name="android:paddingRight">5dp</item>
	</style>

	<style name="chat_text_message_style">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:paddingTop">5dp</item>
		<item name="android:paddingBottom">5dp</item>
		<item name="android:paddingLeft">8dp</item>
		<item name="android:paddingRight">8dp</item>
		<item name="android:textSize">16sp</item>
		<item name="android:textColor">@android:color/black</item>
	</style>

	<style name="chat_timings">
		<item name="android:layout_width">wrap_content</item>
		<item name="android:layout_height">wrap_content</item>
		<item name="android:textSize">12sp</item>
	</style>

	<style name="Theme_Dialog" parent="Theme.AppCompat.Light">
		<item name="android:windowNoTitle">true</item>
		<item name="android:windowBackground">@android:color/transparent</item>
	</style>

	<style name="FlatRedButtonRippleStyle" parent="Material.Drawable.Ripple.Touch.Light">
		<item name="android:background">#EEFF2222</item>
		<item name="rd_backgroundColor">#80c7c7c7</item>
		<item name="rd_rippleColor">#4DFFFFFF</item>
		<item name="rd_cornerRadius">0dp</item>
	</style>

	<style name="FlatGreenButtonRippleStyle" parent="Material.Drawable.Ripple.Touch.Light">
